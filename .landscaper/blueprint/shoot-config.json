{
  "provider": {
    "type": "gcp",
    "zone": "europe-west1-c",
    "workersSettings": {
      "sshAccess": {
        "enabled": false
      }
    }
  },
  "region": "europe-west1",
  "workers": {
    "machine": {
      "type": "n1-standard-2",
      "image": {
        "name": "gardenlinux",
        "version": "1312.3.0"
      }
    },
    "volume": {
      "type": "pd-standard",
      "size": "50Gi"
    },
    "minimum": 1,
    "maximum": 1,
    "maxSurge": 1,
    "maxUnavailable": 0
  },
  "kubernetes": {
    "version": "1.27",
    "kubeAPIServer": {
      "encryptionConfig": {
        "resources": [
          "installations.landscaper.gardener.cloud",
          "executions.landscaper.gardener.cloud",
          "deployitems.landscaper.gardener.cloud",
          "targets.landscaper.gardener.cloud",
          "dataobjects.landscaper.gardener.cloud",
          "contexts.landscaper.gardener.cloud"
        ]
      }
    }
  },
  "maintenance": {
    "timeWindow": {
      "begin": "050000+0200",
      "end": "060000+0200"
    },
    "autoUpdate": {
      "kubernetesVersion": false,
      "machineImageVersion": false
    }
  }
}
